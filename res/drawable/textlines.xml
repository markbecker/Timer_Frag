<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="0dp"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle" >
            <stroke
                android:dashGap="7px"
                android:dashWidth="1px"
                android:width="1dp"
                android:color="#FF666666" />

            <solid android:color="#00000000" />
        </shape>
    </item>

</layer-list>